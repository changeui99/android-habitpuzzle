<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.changeui.habbitpuzzle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_habitpuzzle"
        android:roundIcon="@mipmap/ic_launcher_habitpuzzle_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PuzzleActivityView"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".CollectionSelectActivityView"
            android:screenOrientation="portrait"/>
        <activity android:name=".HabitdetailActivityView"
            android:screenOrientation="portrait"/>
        <activity android:name=".AddActivityView"
            android:screenOrientation="portrait"/>
        <activity android:name=".HabitlistActivityView"
            android:screenOrientation="portrait"/>
        <activity android:name=".startActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivityView"
            android:screenOrientation="portrait" />
    </application>

</manifest>